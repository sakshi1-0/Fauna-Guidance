<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ToRegister</servlet-name>
        <servlet-class>controller.ToRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForSearch</servlet-name>
        <servlet-class>controller.ForSearch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ToUpdate</servlet-name>
        <servlet-class>controller.ToUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ToDelete</servlet-name>
        <servlet-class>controller.ToDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginChecker</servlet-name>
        <servlet-class>controller.LoginChecker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForFeedback</servlet-name>
        <servlet-class>controller.ForFeedback</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ToRegister</servlet-name>
        <url-pattern>/ToRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForSearch</servlet-name>
        <url-pattern>/ForSearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ToUpdate</servlet-name>
        <url-pattern>/ToUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ToDelete</servlet-name>
        <url-pattern>/ToDelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginChecker</servlet-name>
        <url-pattern>/LoginChecker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForFeedback</servlet-name>
        <url-pattern>/ForFeedback</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
